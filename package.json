{
  "name": "llm-trace-lens",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "migrate:latest": "tsx node_modules/knex/bin/cli.js migrate:latest --knexfile src/config/knexfile.ts",
    "migrate:rollback": "tsx node_modules/knex/bin/cli.js migrate:rollback --knexfile src/config/knexfile.ts",
    "migrate:make": "tsx node_modules/knex/bin/cli.js migrate:make --knexfile src/config/knexfile.ts",
    "vercel-build": "npm run build && cd packages/dashboard && npm install && npm run build",
    "vercel-dev": "vercel dev"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@azure/msal-node": "^5.0.4",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "@google/generative-ai": "^0.24.1",
    "@vercel/kv": "^1.0.1",
    "axios": "^1.13.5",
    "better-sqlite3": "^12.6.2",
    "dotenv": "^17.2.4",
    "fastify": "^4.26.0",
    "google-auth-library": "^10.5.0",
    "knex": "^3.1.0",
    "nodemailer": "^8.0.1",
    "openai": "^4.28.0",
    "pdf-lib": "^1.17.1",
    "pg": "^8.18.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.11.0",
    "@types/nodemailer": "^7.0.9",
    "@types/pg": "^8.16.0",
    "@vercel/node": "^3.0.12",
    "@vitest/coverage-v8": "^4.0.18",
    "pino-pretty": "^10.3.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  }
}
